
#include <iostream>
using namespace std;

int main()
{
	// variables
	double number1;
	double number2;
	

	cout << "This program will see which number is bigger.\n";

	//get data
	cout << "Please enter the first number please. \n";
	cin >> number1;

	cout << "Please enter the second number please. \n";
	cin >> number2;

	//decision

	if (number1 > number2)
	{
		cout << "number 1 is bigger" << endl;  
	}
	else
	{
		cout << "number 2 is bigger" << endl; 
	}
	return 0;
}
