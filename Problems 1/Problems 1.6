#include <iostream>
using namespace std;

int main()
{
	// variables being used
	double payAmount = 2200.00;
	double payPeriod = 26;
	double annualPay;

	cout << "Let calculate your paycheck!" << endl;

	annualPay = payAmount * payPeriod; // formula used

	cout << "Your yearly salary is $" << annualPay << endl; // Output of annual salary

	return 0;
}
